<?xml version="1.0" encoding="UTF8"?>
<!DOCTYPE data-def SYSTEM "../dtd/view_file.dtd">
<data-def name="IMAGE_PROCESSOR">
    <group key="detectors">
        <group key="detector">
            <item key="camera"/>
            <item key="dark"/>
            <item key="distance"/>
            <item key="exposureTime"/>
            <item key="shutterCloseDelay"/>
            <item key="xBin"/>
            <item key="yBin"/>
        </group>
        <group key="monitor">
            <item key="mi"/>
            <item key="gain"/>
            <item key="intensity"/>
        </group>
        <group key="monochromator">
            <item key="lambda"/>
            <item key="energy"/>
        </group>
    </group>
    <group key="data">
        <item key="images"/>
        <item key="spectrums"/>
    </group>
    <group key="info">
        <item key="comments"/>
    </group>
</data-def>
